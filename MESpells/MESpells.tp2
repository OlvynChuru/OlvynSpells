COPY_EXISTING_REGEXP ~.*\.spl~ ~override~
	READ_LONG 0x64 headeroffset
	READ_SHORT 0x68 numheaders
	READ_LONG 0x6A effectoffset
	READ_SHORT (headeroffset + 0x28 * (numheaders - 1) + 0x1E) lastheadernumeffects
	READ_SHORT (headeroffset + 0x28 * (numheaders - 1) + 0x20) lastheaderfirsteffectindex
	numeffects = lastheadernumeffects + lastheaderfirsteffectindex
	offset = effectoffset
	doesnotstack = 0
	dicestoneskin = 0
	FOR (i = 0; i < numeffects; ++i) BEGIN
		READ_SHORT offset theopcode ELSE 0
		PATCH_IF theopcode = stoneskin BEGIN
			READ_LONG (offset + 0x4) theparameter1 ELSE 0
			READ_LONG (offset + 0x8) theparameter2 ELSE 0
			READ_LONG (offset + 0x1C) thedicenumber ELSE 0
			READ_LONG (offset + 0x20) thedicesize ELSE 0
			READ_LONG (offset + 0x2C) thespecial ELSE 0
			PATCH_IF thespecial = 0 BEGIN
				thespecial = 80
			END
			PATCH_IF doesnotstack = 0 BEGIN
				LPF CLONE_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=removeeffectsbyresource parameter1=0 parameter2=0 dicenumber=0 dicesize=0 STR_VAR resource= EVAL ~%SOURCE_RES%~ END
			END
			LPF CLONE_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=displayportraiticon parameter2=thespecial dicenumber=0 dicesize=0 END
			LPF CLONE_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=401 parameter2=0 special=635 dicenumber=0 dicesize=0 END
			PATCH_IF theparameter2 = 1 AND thedicenumber = 1 AND thedicesize = 4 BEGIN
				dicestoneskin = 1
				LPF CLONE_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=403 parameter2=0x4990 special=0 probability1=24 probability2=0 dicenumber=0 dicesize=0 STR_VAR resource=~MESTONES~ END
				LPF CLONE_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=403 parameter2=0x4990 special=0 probability1=49 probability2=25 dicenumber=0 dicesize=0 STR_VAR resource=~MESTONES~ END
				LPF CLONE_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=403 parameter2=0x4990 special=0 probability1=74 probability2=50 dicenumber=0 dicesize=0 STR_VAR resource=~MESTONES~ END
				LPF ALTER_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=403 parameter2=0x4990 special=0 probability1=100 probability2=75 dicenumber=0 dicesize=0 STR_VAR resource=~MESTONES~ END
			END ELSE BEGIN
				LPF ALTER_EFFECT INT_VAR silent=1 check_headers=1 check_globals=1 match_opcode=stoneskin opcode=403 parameter2=0x4990 special=0 dicenumber=0 dicesize=0 STR_VAR resource=~MESTONES~ END
			END
		END ELSE PATCH_IF theopcode = removeeffectsbyresource BEGIN
			READ_ASCII (offset + 0x14) theresource ELSE ~~
			PATCH_IF (~%theresource%~ STRING_EQUAL_CASE ~%SOURCE_RES%~) BEGIN
				doesnotstack = 1
			END
		END
		offset = offset + 0x30
	END
	PATCH_IF dicestoneskin = 1 BEGIN
		READ_LONG 0x64 headeroffset
		READ_SHORT 0x68 numheaders
		READ_LONG 0x6A effectoffset
		READ_SHORT (headeroffset + 0x28 * (numheaders - 1) + 0x1E) lastheadernumeffects
		READ_SHORT (headeroffset + 0x28 * (numheaders - 1) + 0x20) lastheaderfirsteffectindex
		numeffects = lastheadernumeffects + lastheaderfirsteffectindex
		offset = effectoffset
		FOR (i = 0; i < numeffects; ++i) BEGIN
			READ_SHORT offset theopcode ELSE 0
			READ_ASCII (offset + 0x14) theresource ELSE ~~
			PATCH_IF theopcode = 403 AND (~%theresource%~ STRING_EQUAL_CASE ~MESTONES~) BEGIN
				READ_LONG (offset + 0x4) theparameter1 ELSE 0
				READ_BYTE (offset + 0x12) theprobability1 ELSE 0
				READ_BYTE (offset + 0x13) theprobability2 ELSE 0
				PATCH_IF theprobability1 = 24 AND theprobability2 = 0 BEGIN
					theparameter1 += 1
				END ELSE PATCH_IF theprobability1 = 49 AND theprobability2 = 25 BEGIN
					theparameter1 += 2
				END ELSE PATCH_IF theprobability1 = 74 AND theprobability2 = 50 BEGIN
					theparameter1 += 3
				END ELSE PATCH_IF theprobability1 = 100 AND theprobability2 = 75 BEGIN
					theparameter1 += 4
				END
				WRITE_LONG (offset + 0x4) theparameter1
			END
			offset = offset + 0x30
		END
	END
	BUT_ONLY_IF_IT_CHANGES


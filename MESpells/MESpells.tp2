COPY_EXISTING_REGEXP ~.*\.spl~ ~override~
	READ_SHORT 0x1C thespelltype
	PATCH_IF thespelltype = 1 OR thespelltype = 2 BEGIN
		LPF ADD_SPELL_CFEFFECT INT_VAR opcode=eeexinvokelua target=1 timing=0 STR_VAR resource=~EXMETALV~ END
	END
	UNLESS ~EXMETALV~
	BUT_ONLY_IF_IT_CHANGES

COPY_EXISTING_REGEXP ~.*\.spl~ ~override~
	READ_BYTE 0x7E thetarget ELSE 0
	READ_SHORT 0x98 theprojectile ELSE 0
	PATCH_IF thetarget = 5 AND (theprojectile = 1 OR theprojectile = 66 OR theprojectile = 195 OR theprojectile = 255) BEGIN
//		LPF ALTER_EFFECT INT_VAR silent=1 check_headers=1 check_globals=0 match_target=1 target=2 END
		READ_LONG 0x64 headeroffset
		READ_SHORT 0x68 numheaders
		READ_LONG 0x6A effectoffset
		READ_SHORT 0x70 numglobaleffects
		READ_SHORT (headeroffset + 0x28 * (numheaders - 1) + 0x1E) lastheadernumeffects ELSE 0
		READ_SHORT (headeroffset + 0x28 * (numheaders - 1) + 0x20) lastheaderfirsteffectindex ELSE 0
		numeffects = lastheadernumeffects + lastheaderfirsteffectindex
		offset = effectoffset + numglobaleffects * 0x30
		FOR (i = lastheaderfirsteffectindex; i < numeffects; ++i) BEGIN
			READ_BYTE (offset + 0x2) thetarget ELSE 0
			PATCH_IF thetarget = 1 BEGIN
				WRITE_BYTE (offset + 0x2) 2
			END
			offset = offset + 0x30
		END
	END
	BUT_ONLY_IF_IT_CHANGES


				REPLACE_TEXTUALLY CASE_SENSITIVE ~%match_savingthrow%~ ~%new_savingthrow%~
			END
		END
		PATCH_IF new_string = 0 BEGIN
			INNER_ACTION BEGIN
				STRING_SET_EVALUATE description_ref ~%description_new%~
			END
		END
		ELSE BEGIN
